	if(isMobile == true){
		no_months = 1;
	}
		var obj = new Array(215-214,178-214,214-215,4688-214,4688-4839,214-178,5401-4276,215-4839,4839-215,215-4688,214-4688,215-163);
	$.each(obj, function(i, item) {
			$("#bookdatepicker"+i).datepicker({dateFormat: 'dd-mm-yy', firstDay: 1, numberOfMonths: no_months, showButtonPanel: false, minDate: +0, maxDate: '+1M +15D',
			onSelect: function(selectedDate) {
				var strr = $(this).attr("url");
				strr = strr.replace("BUSROUTEDATE", selectedDate);
				strr = strr.replace(/[() ]/g, '');
				var parameters = strr.split("bus_search");				
				var paramvalues =  parameters[1].split("/");
				var refineParam = "";

				if (paramvalues[6] != '') {
					var refine = paramvalues[6].split("?");
					refine = refine[1];
					refineParam = '&'+refine;
				}
				setCookie("source_id", paramvalues[2], 2);
				setCookie("destination_id", paramvalues[4], 2);
				setCookie("source", paramvalues[1], 2);
				setCookie("destination", paramvalues[3], 2);
		
			if(isMobile == true){
				var url = parameters[0]+"mobile/search?action=search&jdate="+selectedDate+"&srcid="+paramvalues[2]+"&destid="+paramvalues[4]+"&srcname="+paramvalues[1]+"&destname="+paramvalues[3]+refineParam+"&landedFrom=operatordescnew";
					

					url +='&refineid='+111316;
								
				window.location= url;
				return; 
			}

				strr +='&refineid='+111316;
				

				window.location= strr;
			}
			}).click(function () { $(this).focus(); });
		});
		var HeadTxt = "OSRTC"
	$('#searchHeadTitle').html('Search '+HeadTxt+' Bus Tickets'); 	     
});

function setCookie(cname, cvalue, exdays) {
  var d = new Date();
  d.setTime(d.getTime() + (exdays*24*60*60*1000));
  var expires = "expires="+ d.toUTCString();
  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

$("#DpHref").on('click', function() {
		$('.ratingMore').show();
		$('#DpHref').hide();
		$('#DpShowLess').show();
	});
$("#DpShowLess").on('click', function() {
		$('.ratingMore').hide();
		$('#DpShowLess').hide();
		$('#DpHref').show();
});


