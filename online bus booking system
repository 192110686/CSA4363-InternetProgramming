<script>
function openModal(id){
	var modal = document.getElementById('modal-demo'+id);
	 modal.classList.toggle('visible');
}
function closeModal(id){
	var modal = document.getElementById('modal-demo'+id);
	 modal.classList.remove('visible');
}
</script>
 
									 
